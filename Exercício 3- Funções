print('Exercício 3')

def maximo():
    p=0     
    x=[]
    while p<5:
        y=int(input('Digite um número: '))
        x.append(y)
        p+=1
        print(x)

    
    ##while t<4:
    for t in x:
        t=0        
        mult=(x[t])*(x[t-1])*(x[t-2])*(x[t-3])*(x[t-4])
        
   
    print('A multiplicação da nossa lista é',mult)


maximo()
